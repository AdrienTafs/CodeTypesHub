{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "camelcase": ["error", { "properties": "never" }],
      "quotes": ["error", "backtick"],
      "indent": ["error", 4],
      "prefer-const": "error",
      "no-console": "warn",
      "arrow-body-style": ["error", "as-needed"]
    }
  }
  